<%= simple_form_for user do |f| %>
 
  <%= f.input :postcode %><br>
  <%= f.input :prefecture_code, collection: JpPrefecture::Prefecture.all, :value_method => :name, include_blank: '都道府県' %><br>
  <%= f.input :address_city %><br>
  <%= f.input :address_street %><br>
  <%= f.input :address_building %><br>
<% end %>